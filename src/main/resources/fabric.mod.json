{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [ "${mod_authors}" ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_sources}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "assets/slotcars/icon.png",
  "environment": "*",
  "accessWidener": "slotcars.accesswidener",
  "entrypoints": {
    "client": [
      "dev.chililisoup.slotcars.client.SlotCarsClient"
    ],
    "main": [
      "dev.chililisoup.slotcars.SlotCars"
    ]
  },
  "mixins": [
    "slotcars.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "~${minecraft_version}"
  }
}
